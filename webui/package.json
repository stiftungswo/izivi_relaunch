{
  "name": "izivi-webui",
  "version": "0.0.1",
  "private": true,
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "graphql": "^0.9.3",
    "graphql-tag": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "meteor-apollo-accounts": "^2.0.1",
    "next": "latest",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-apollo": "^1.0.2",
    "react-dom": "^15.5.4",
    "recompose": "^0.23.1",
    "semantic-ui-react": "^0.68.0",
    "simpl-schema": "^0.2.3",
    "uniforms": "^1.17.2",
    "uniforms-semantic": "^1.17.2"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "import",
      "jsx-a11y",
      "react"
    ],
    "extends": [
      "airbnb"
    ],
    "globals": {
      "server": false,
      "browser": false,
      "expect": false
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "react/prop-types": [
        0
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  }
}
