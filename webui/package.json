{
  "name": "izivi-webui",
  "version": "0.0.1",
  "private": true,
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "GRAPHQL_ENDPOINT=http://localhost:3010/graphql next",
    "build": "GRAPHQL_ENDPOINT=http://localhost:3010/graphql next build",
    "start": "next start"
  },
  "dependencies": {
    "apollo-client": "^1.2.2",
    "classnames": "^2.2.5",
    "graphql": "^0.9.6",
    "graphql-tag": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "meteor-apollo-accounts": "^2.0.1",
    "moment": "^2.18.1",
    "next": "^2.3.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-apollo": "^1.2.0",
    "react-datepicker": "^0.46.0",
    "react-dom": "^15.5.4",
    "react-dropzone": "^3.13.1",
    "react-input-mask": "^0.8.0",
    "react-phone-number-input": "^0.11.2",
    "recompose": "^0.23.4",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.3",
    "simpl-schema": "^0.3.0",
    "uniforms": "^1.18.0",
    "uniforms-semantic": "^1.18.0"
  },
  "devDependencies": {
    "babel-plugin-transform-inline-environment-variables": "0.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "import",
      "jsx-a11y",
      "react"
    ],
    "extends": [
      "airbnb"
    ],
    "globals": {
      "server": false,
      "browser": false,
      "expect": false
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "react/prop-types": [
        0
      ],
      "react/jsx-wrap-multilines": [
        0
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "__",
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  }
}
