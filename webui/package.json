{
  "name": "webui",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "apollo-client": "^1.0.2",
    "babel-runtime": "6.23.0",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "graphql": "^0.9.2",
    "graphql-server-express": "^0.7.0",
    "graphql-tools": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-graphql": "^0.7.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ],
    "plugins": [
      "babel-plugin-inline-import"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "meteor",
      "import",
      "graphql"
    ],
    "extends": [
      "airbnb-base",
      "plugin:meteor/recommended"
    ],
    "globals": {
      "server": false,
      "browser": false,
      "expect": false
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "settings": {
      "import/core-modules": [
        "meteor/*"
      ],
      "import/resolver": {
        "meteor": {
          "extensions": [
            ".jsx",
            ".js"
          ]
        }
      }
    },
    "rules": {
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Maybe",
            "ReactiveAggregate"
          ]
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "import/extensions": [
        "off",
        "never"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "__",
            "_id",
            "_ensureIndex",
            "_verifyEmailToken",
            "_resetPasswordToken",
            "_name"
          ]
        }
      ]
    }
  }
}
